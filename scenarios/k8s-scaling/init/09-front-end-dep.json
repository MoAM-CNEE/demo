{
  "collectionName": "moam.statemanager",
  "actionName": "CreateEntityAction",
  "params": {
    "definition": "{\n  \"apiVersion\": \"kubernetes.crossplane.io/v1alpha2\",\n  \"kind\": \"Object\",\n  \"metadata\": {\n    \"name\": \"09-front-end-dep\"\n  },\n  \"spec\": {\n    \"forProvider\": {\n      \"manifest\": {\n        \"apiVersion\": \"apps/v1\",\n        \"kind\": \"Deployment\",\n        \"metadata\": {\n          \"name\": \"front-end\",\n          \"namespace\": \"sock-shop\"\n        },\n        \"spec\": {\n          \"replicas\": 1,\n          \"selector\": {\n            \"matchLabels\": {\n              \"name\": \"front-end\"\n            }\n          },\n          \"template\": {\n            \"metadata\": {\n              \"labels\": {\n                \"name\": \"front-end\"\n              }\n            },\n            \"spec\": {\n              \"containers\": [\n                {\n                  \"name\": \"front-end\",\n                  \"image\": \"weaveworksdemos/front-end:0.3.12\",\n                  \"resources\": {\n                    \"limits\": {\n                      \"cpu\": \"300m\",\n                      \"memory\": \"1000Mi\"\n                    },\n                    \"requests\": {\n                      \"cpu\": \"100m\",\n                      \"memory\": \"300Mi\"\n                    }\n                  },\n                  \"ports\": [\n                    {\n                      \"containerPort\": 8079\n                    }\n                  ],\n                  \"env\": [\n                    {\n                      \"name\": \"SESSION_REDIS\",\n                      \"value\": \"true\"\n                    }\n                  ],\n                  \"securityContext\": {\n                    \"runAsNonRoot\": true,\n                    \"runAsUser\": 10001,\n                    \"capabilities\": {\n                      \"drop\": [\n                        \"all\"\n                      ]\n                    },\n                    \"readOnlyRootFilesystem\": true\n                  },\n                  \"livenessProbe\": {\n                    \"httpGet\": {\n                      \"path\": \"/\",\n                      \"port\": 8079\n                    },\n                    \"initialDelaySeconds\": 300,\n                    \"periodSeconds\": 3\n                  },\n                  \"readinessProbe\": {\n                    \"httpGet\": {\n                      \"path\": \"/\",\n                      \"port\": 8079\n                    },\n                    \"initialDelaySeconds\": 30,\n                    \"periodSeconds\": 3\n                  }\n                }\n              ],\n              \"nodeSelector\": {\n                \"beta.kubernetes.io/os\": \"linux\"\n              }\n            }\n          }\n        }\n      }\n    },\n    \"providerConfigRef\": {\n      \"name\": \"provider-config-kubernetes\"\n    }\n  }\n}"
  }
}
