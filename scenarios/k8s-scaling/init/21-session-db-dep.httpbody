    "definition": "{\n  \"apiVersion\": \"kubernetes.crossplane.io/v1alpha2\",\n  \"kind\": \"Object\",\n  \"metadata\": {\n    \"name\": \"21-session-db-dep\"\n  },\n  \"spec\": {\n    \"forProvider\": {\n      \"manifest\": {\n        \"apiVersion\": \"apps/v1\",\n        \"kind\": \"Deployment\",\n        \"metadata\": {\n          \"name\": \"session-db\",\n          \"labels\": {\n            \"name\": \"session-db\"\n          },\n          \"namespace\": \"sock-shop\"\n        },\n        \"spec\": {\n          \"replicas\": 1,\n          \"selector\": {\n            \"matchLabels\": {\n              \"name\": \"session-db\"\n            }\n          },\n          \"template\": {\n            \"metadata\": {\n              \"labels\": {\n                \"name\": \"session-db\"\n              },\n              \"annotations\": {\n                \"prometheus.io/scrape\": \"false\"\n              }\n            },\n            \"spec\": {\n              \"containers\": [\n                {\n                  \"name\": \"session-db\",\n                  \"image\": \"redis:alpine\",\n                  \"ports\": [\n                    {\n                      \"name\": \"redis\",\n                      \"containerPort\": 6379\n                    }\n                  ],\n                  \"securityContext\": {\n                    \"capabilities\": {\n                      \"drop\": [\n                        \"all\"\n                      ],\n                      \"add\": [\n                        \"CHOWN\",\n                        \"SETGID\",\n                        \"SETUID\"\n                      ]\n                    }\n                  }\n                }\n              ],\n              \"nodeSelector\": {\n                \"beta.kubernetes.io/os\": \"linux\"\n              }\n            }\n          }\n        }\n      }\n    },\n    \"providerConfigRef\": {\n      \"name\": \"provider-config-kubernetes\"\n    }\n  }\n}"
